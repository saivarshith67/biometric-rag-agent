# Example environment variables for Biometric RAG Agent
# Copy this file to .env and fill in your actual values

# Google Gemini API key
GOOGLE_API_KEY=

# ChromaDB connection string (if using remote DB)
DB_URL=

# Langfuse observability
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST_URL=

# LlamaParse Cloud API key
LLAMA_CLOUD_API_KEY=

# (Optional) HuggingFace or OpenAI API keys for LLMs
HUGGINGFACE_API_KEY=
OPENAI_API_KEY=

# (Optional) Other configuration
# DEBUG=True
# LOG_LEVEL=INFO
